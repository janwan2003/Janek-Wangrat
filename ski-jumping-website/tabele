DROP TABLE Zgloszenie;
CREATE TABLE Zgloszenie(
id PRIMARY KEY,
data_zgloszenia DATE NOT NULL ,
id_zawodnika INTEGER NOT NULL REFERENCES Zawodnik,
id_konkursu INTEGER NOT NULL REFERENCES Konkurs
);
DROP TABLE Zawodnik;
CREATE TABLE Zawodnik(
id INTEGER PRIMARY KEY,
imie VARCHAR(32) NOT NULL,
nazwisko VARCHAR(32) NOT NULL,
kraj VARCHAR(32) NOT NULL REFERENCES Kraj
);
DROP TABLE Skok;
CREATE TABLE Skok(
id INTEGER PRIMARY KEY,
dlugosc INTEGER NOT NULL,
ocena INTEGER NOT NULL,
seria INTEGER NOT NULL,
id_zawodnika INTEGER NOT NULL REFERENCES Zawodnik
);
DROP TABLE Kraj;
CREATE TABLE Kraj(
nazwa VARCHAR(32) PRIMARY KEY,
kwota_startowa INTEGER NOT NULL
);
DROP TABLE Konkurs;
CREATE TABLE Konkurs(
id INTEGER PRIMARY KEY,
nazwa VARCHAR(32) NOT NULL,
data_konkursu DATE NOT NULL,
termin_zgloszen DATE NOT NULL
);